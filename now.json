{
  "version": 2,
  "name": "stimcraft",
  "builds": [
    {
      "src": "/api/api.py",
      "use": "@now/python"
    },
    {
      "src": "now.sh",
      "use": "@now/static-build",
      "config": { "distDir": "front" }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/.*",
      "dest": "/api/api.py"
    }
  ]
}
